@html.extend('layout', function (model) {
@html.block('body', function (model) {

<div class="container">

    <div class="row row-offcanvas row-offcanvas-right">

        <h1>User details</h1>

        <ul>
            <li>@model.refUser.name</li>
            <li>@model.refUser.username</li>
            <li>@model.refUser.email</li>
        </ul>

    </div>
</div>

})
})